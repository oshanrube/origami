{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es6",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "paths": {
      "@oshanrube/origami": ["dist/"]
    },
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
}
